{
  "name": "kiwi-webpack-plugin",
  "version": "0.0.6",
  "description": "Test Runner for Webpack and the Kakoune editor",
  "main": "dist/main.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "webpack --mode development -w",
    "build": "webpack --mode production",
    "test": "(cd examples/minimal/ && npm run build) && webpack-dev-server --config tests-webpack.config.js",
    "cleanup": "rm dist/*_tests.js review_app/venv examples -rf"
  },
  "keywords": [
    "kiwi",
    "test-runner",
    "tests",
    "javascript",
    "typescript",
    "kakoune",
    "kakoune-plugin",
    "webpack",
    "webpack-plugin",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoand/kiwi-webpack-plugin.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoand/kiwi/issues"
  },
  "homepage": "https://github.com/stoand/kiwi#readme",
  "dependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.5",
    "@types/source-map": "^0.5.7",
    "@types/webpack": "^4.41.7",
    "chai": "^4.2.0",
    "chrome-launcher": "^0.13.0",
    "chrome-remote-interface": "^0.28.1",
    "raw-loader": "^4.0.0",
    "source-map": "^0.7.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "v8-to-istanbul": "^4.1.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
